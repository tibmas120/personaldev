.slo-common-form{
    color:#111;
}
.slo-common-form fieldset > legend{
    border-bottom:1px solid #ccc;
    color:#666;
    font-size:36px;
    font-family:"FFDINWeb";
    line-height:1;
    padding-bottom:10px;
    margin-bottom:0;
}
.slo-common-form .form-group{
    position:relative;
    margin-bottom:0;
}
.slo-common-form label.info{    
    font-size:14px;
    line-height:1;
    font-weight:normal;
    font-family:"FFDINWebItalic";
    display:block;
}
.slo-common-form label[aria-labelledby=title]{    
    font-size:18px;
    font-family:"FFDINWebBold";
    margin:0;
    padding:0 0 20px;
    height:38px;
    display:block;
    text-transform:uppercase;
    line-height: 1;
    font-weight: normal;
}
.slo-common-form label[aria-labelledby=title] > span.fa-question-circle{
    color:#00517A;
   
}
.slo-common-form label[aria-labelledby=title] + label.info{
    margin-top:-10px;
    margin-bottom:10px;
    line-height:1.75;
    letter-spacing:0.5px;
}

.slo-common-form.reviewForm label[aria-labelledby=title]{
    margin:16px 0 0;
}
.slo-common-form.reviewForm label[aria-labelledby=value]{
    font-weight:normal;
    display:block;
    margin-bottom:0;
}

.slo-common-form .form-control{
    border-radius:0;
    font-size:16px;
}
.slo-common-form .form-control::-moz-placeholder {
  font-family:"FFDinWebItalic";
}
.slo-common-form .form-control:-ms-input-placeholder {
  font-family:"FFDinWebItalic";
}
.slo-common-form .form-control::-webkit-input-placeholder {
  font-family:"FFDinWebItalic";
}
.slo-common-form input[type=email],
.slo-common-form input[type=text]{
    color:#111;
    padding: 10px 15px;
    min-height:50px;
}
.slo-common-form .form-control.hasDatepicker{
    background-image: url(../../img/icon-calendar.png);
    background-position: center right 10px;
    background-repeat: no-repeat;
}
/*validaion interaction start*/
.slo-common-form input[type=email].valid, 
.slo-common-form input[type=text].valid {
    border: 2px solid #14993F;
    background-image: url(../../img/icon-form-sucess.png);
    background-position: center right 10px;
    background-repeat: no-repeat;
    padding-right: 50px;
}
.slo-common-form input[type=text]#phNo{
    border: 1px solid #ccc;
    background: none;
    padding-right: 50px
}

.slo-common-form input[type=email].error, 
.slo-common-form input[type=text].error {
    border: 2px solid #B02621;
    background-image: url(../../img/icon-form-error.png);
    background-position: center right 10px;
    background-repeat: no-repeat;
    padding-right: 42px;
}

.slo-common-form input#phNo{
    border: 1px solid #ccc;
    background: none;
    padding-right: 0;
}
.slo-common-form input#phNo[aria-invalid=true]{
    border: 2px solid #B02621;
    background-image: url(../../img/icon-form-error.png);
    background-position: center right 10px;
    background-repeat: no-repeat;
    padding-right: 42px;
}
.slo-common-form input#phNo[aria-invalid=false]{
    border: 1px solid #ccc;
    background-image: none;
    background-position: center right 10px;
    background-repeat: no-repeat;
    padding-right: 50px;
}
.slo-common-form .form-group label.error {
    margin-top: 34px;
}
.slo-common-form .form-group.altFormGroup label.error {
    float: left;
    width: 30%;
    display: block;
    margin-top: 23px;
    margin-left: 103%;
    position: absolute;
    bottom: 0px;
}
.slo-common-form label.error{
    font-size:14px;
    font-family: 'FFDINWeb';
    font-weight:normal;
    margin-bottom:0;
}
.slo-common-form label.error:before {
    content: "REQUIRED";
    color: #B02621;
    position: absolute;
    margin-top: -24px;
    font-family: 'FFDINWebBold';
    font-weight: normal;
}

/*validaion interaction ends*/

/*cal + jq ui modification as per theme start*/
.ui-widget{
    font-family: "FFDINWeb";
}
.ui-widget-content{border:1px solid #ccc;color:#666;}
.ui-state-highlight, 
.ui-widget-content .ui-state-highlight, 
.ui-widget-header .ui-state-highlight,
.ui-state-default, 
.ui-widget-content .ui-state-default, 
.ui-widget-header .ui-state-default{
    text-align:center;
}
/*cal + jq ui modification as per theme ends*/

/*grouping for two fields/three dropdown start*/
.slo-common-form .form-group.twoFieldHandler:after{
    clear:both;
    content:'';
    display:block;
}
.slo-common-form .form-group.twoFieldHandler > section{
    display:inline-table;
    width:calc(50% - 10px);
}
.slo-common-form .form-group.twoFieldHandler > section:first-of-type{
    float:left; 
}
.slo-common-form .form-group.twoFieldHandler > section:last-of-type{
    float:right;
}
.slo-common-form .form-group.multiDropdownHandler:after{
    clear:both;
    content:'';
    display:table;
}
.slo-common-form .form-group.multiDropdownHandler > section{    
    display:inline-table;
    float:left;
    width: 33.33333333333333%;        
}
.slo-common-form .form-group.multiDropdownHandler > section:first-child {
    padding-right:10px;
}
.slo-common-form .form-group.multiDropdownHandler > section:nth-child(2) {
    padding:0 5px 0;
}
.slo-common-form .form-group.multiDropdownHandler > section:last-child {
    padding-left:10px;
}


.slo-common-form .form-group .dropdown > select.btn{
    color:#999;
    width:100%;
    border-radius:0; 
    font-size:16px;
    /*font-style:italic;*/
    text-align:left;
    /*padding: 10px 15px; */
	padding: 10px 30px 10px 15px;
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    background: url(../../img/form_select.png) right center no-repeat;
    min-height: 50px;
}

.slo-common-form .form-group .dropdown > select.btn::-ms-expand{
	display:none;
}

.slo-common-form .form-group .dropdown > select.btn:active:hover, 
.slo-common-form .form-group .dropdown > select.btn.active:hover, 
.slo-common-form .form-group .dropdown.open > .dropdown-toggle.btn-default:hover, 
.slo-common-form .form-group .dropdown > select.btn:active:focus, 
.slo-common-form .form-group .dropdown > select.btn.active:focus, 
.slo-common-form .form-group .dropdown.open > .dropdown-toggle.btn-default:focus, 
.slo-common-form .form-group .dropdown > select.btn:active.focus, 
.slo-common-form .form-group .dropdown > select.btn.active.focus, 
.slo-common-form .form-group .dropdown.open > .dropdown-toggle.btn-default.focus,
.slo-common-form .form-group .dropdown > select.btn:hover,
.slo-common-form .form-group .dropdown > select.btn:focus{
    color:#999;
    background-color: transparent;
    border:1px solid #ccc;
}
.slo-common-form .form-group .dropdown > select.btn > option{
    color:#666;
    font-style:normal;
}
