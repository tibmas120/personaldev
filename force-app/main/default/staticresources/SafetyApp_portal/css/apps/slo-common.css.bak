        margin-top: 35px;
    }

    .slo-common-spacings_vs3 {
        margin-top: 20px;
    }
    /*spacers ends*/
}

.article-header,.listing-header {
    position: relative;
    text-align: center;
    color: #fff;
    margin: 0
}

.article-header .header-content,.listing-header .header-content {
    display: table;
    width: 100%;
    margin: 0
}

.article-header .wrapper,.listing-header .wrapper {
    display: table-cell;
    vertical-align: middle
}

.article-header .wrapper .h1,.listing-header .wrapper .h1 {
    margin-bottom: 0;
    margin-top: 0;
    font-family: FFDINWeb,Arial;
    font-weight: 400;
    font-style: normal;
    text-transform: none
}
.jaws-hidden{
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0, 0, 0, 0);
    border: 0;
}
.article-header .wrapper .header-content-subtitle,.listing-header .wrapper .header-content-subtitle {
    font-size: 14px;
    font-family: FFDINWebBold,Arial;
    font-weight: 400;
    font-style: normal;
    text-transform: uppercase;
    line-height: inherit
}

.listing-header.listingTabs .wrapper .heading {
    margin-top: -52px;
    font-size:60px;
}
.header-content .heading{
    font-size:60px;
}
@media (max-width: 991px) {
    .header-content .heading {
        font-size:30px;
    }
}
@media (max-width: 767px) {
    .listing-header.listingTabs .wrapper .heading {
        margin-top:0
    }
}

.article-header {
    background-color: #07679B
}

.article-header .hero-image-bg {
    position: absolute;
    left: 0;
    width: 100%;
    text-align: center;
    overflow: hidden;
    background-color: #000
}

.article-header .hero-image-bg img {
    opacity: .7;
    position: relative;
    width: 1680px
}

@media (max-width: 1680px) {
    .article-header .hero-image-bg img {
        left:50%;
        -webkit-transform: translate(-50%,0);
        transform: translate(-50%,0)
    }
}

@media (max-width: 767px) {
    .article-header .hero-image-bg img {
        height:150px;
        width: 767px
    }
}

.article-header .header-content {
    height: 350px
}

@media (max-width: 767px) {
    .article-header .header-content {
        height:150px
    }
}

.listing-header {
    background-color: #07679B;
}

.listing-header .header-content {
    height: 250px
}

@media (max-width: 767px) {
    .listing-header .header-content {
        height:150px
    }
}

.listing-header .l3-image {
    background-image: url(../../img/headding_texture.jpg);
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat
}

@media (min-width: 768px) and (-webkit-min-device-pixel-ratio:2),(min-width:768px) and (min--moz-device-pixel-ratio:2),(min-width:768px) and (-o-min-device-pixel-ratio:2/1),(min-width:768px) and (min-device-pixel-ratio:2),(min-width:768px) and (min-resolution:192dpi),(min-width:768px) and (min-resolution:2dppx) {
    .listing-header .l3-image {
        background-image:url(../../img/headding_texture.jpg)
    }
}
