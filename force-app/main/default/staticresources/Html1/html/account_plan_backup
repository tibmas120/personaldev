    <script>   
    function addRow(){
            if ($("table textarea").length > 0) {
            $("table textarea").resizable();
            }
            if ($("table .datepick").length > 0) {
                $('table .input-group.datepick').datepicker({
                    
                    calendarWeeks: false,
                    todayHighlight: true,
                    autoclose: true                   
                });
            }
            $(".datepick input").on("keypress touchstart",function(e){
                    e.preventDefault();                    
                });
        
            if ($("table .close-datepick").length > 0) {
                $('table .input-group.close-datepick').datepicker({
                    calendarWeeks: false,
                    todayHighlight: true,
                    autoclose: true,
                    startDate: '+d'                    
                });
             }
                $(".close-datepick input").on("keypress touchstart",function(e){
                    e.preventDefault();                    
                }); 
           
        }
       
    </script>
     <script>
    function openCity(evt, cityName) {
    
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
        }
        document.getElementById(cityName).style.display = "block";
        evt.currentTarget.className += " active";
    }
    
    // Get the element with id="defaultOpen" and click on it
    document.getElementById("defaultOpen").click();
    </script>
    <script>
    function openPage(cityName, pageId) {    
 
        var i, tabcontent, tablinks, d;
        tabcontent = document.getElementsByClassName("tabcontent");
         
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace("active", "");
        }
        document.getElementById(cityName).style.display = "block";
        if(pageId == 'Page2Tab'){
            d = document.getElementById("Page2Tab");

        } else if (pageId == 'Page3Tab'){
            d = document.getElementById("Page3Tab");
        } else {
            if (pageId == 'Page1Tab')
            {
            d = document.getElementById("defaultOpen");
            }
        }
        d.className += " active";

    }  
    
    
    
    </script>
    
<script> 
(function(){try{var a=navigator.userAgent; 
if((a.indexOf('Salesforce')!=-1)&&(a.indexOf('iPhone')!=-1||a.indexOf('iPad')!=-1)&&(a.indexOf('OS/8')!=-1||a.indexOf('OS 8')!=-1)&&(a.indexOf('Safari')==-1)){ 
var s=document.createElement('style'); 
s.innerHTML="html,html body{overflow: auto;-webkit-overflow-scrolling:touch;}body{position:absolute;left:0;right:0;top:0;bottom:0;}"; 
document.getElementsByTagName('head')[0].appendChild(s);}}catch(e){}})(); 
</script> 
</body>
</html>

